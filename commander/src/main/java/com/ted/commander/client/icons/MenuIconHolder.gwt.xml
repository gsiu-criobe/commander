<module>
    <replace-with class="com.ted.commander.client.icons.MenuIconHolderDefaultAppearance">
        <when-type-is class="com.ted.commander.client.icons.MenuIconHolder.IconHolderAppearance"/>
    </replace-with>
    <replace-with class="com.ted.commander.client.icons.MenuIconHolderDefaultHighAppearance">
        <when-type-is class="com.ted.commander.client.icons.MenuIconHolder.IconHolderAppearance"/>
        <any>
            <when-property-is name="mgwt.density" value="high"/>
        </any>
    </replace-with>
    <replace-with class="com.ted.commander.client.icons.MenuIconHolderDefaultXHighAppearance">
        <when-type-is class="com.ted.commander.client.icons.MenuIconHolder.IconHolderAppearance"/>
        <any>
            <when-property-is name="mgwt.density" value="xhigh"/>
        </any>
    </replace-with>
</module>