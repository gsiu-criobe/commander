<module>
    <replace-with class="com.ted.commander.client.icons.ApplicationIconHolderDefaultAppearance">
        <when-type-is class="com.ted.commander.client.icons.ApplicationIconHolder.IconHolderAppearance"/>
    </replace-with>
    <replace-with class="com.ted.commander.client.icons.ApplicationIconHolderDefaultHighAppearance">
        <when-type-is class="com.ted.commander.client.icons.ApplicationIconHolder.IconHolderAppearance"/>
        <any>
            <when-property-is name="mgwt.density" value="high"/>
        </any>
    </replace-with>
    <replace-with class="com.ted.commander.client.icons.ApplicationIconHolderDefaultXHighAppearance">
        <when-type-is class="com.ted.commander.client.icons.ApplicationIconHolder.IconHolderAppearance"/>
        <any>
            <when-property-is name="mgwt.density" value="xhigh"/>
        </any>
    </replace-with>
</module>