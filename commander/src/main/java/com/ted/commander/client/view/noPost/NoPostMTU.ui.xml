<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">



    <ui:style>
        .max {
            width: 100%;
        }
    </ui:style>
    <g:HTMLPanel addStyleNames="{style.max}">
        <div class="horizontal layout center wrap" style="margin-left:64px">
            <div class="paper-font-body1" ui:field="mtuName" style="width:80px"/>
            <div class="paper-font-body1" ui:field="lastPostDate" style="width:120px"></div>
            <div class="paper-font-body1" ui:field="mtuDescription"></div>
        </div>
    </g:HTMLPanel>

</ui:UiBinder>