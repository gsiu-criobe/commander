<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">

    <ui:with field="style" type="com.petecode.common.client.widget.paper.PaperStyleBundle"/>
    <ui:with field="stringRes" type="com.ted.commander.client.resources.WebStringResource"/>
    <ui:with field="imageRes" type="com.ted.commander.client.resources.DefaultImageResource"/>

    <ui:style field="localStyle" src="../../../style/commanderStyle.gss"
              type="com.ted.commander.client.style.CommanderStyle" gss="true">

        .captionField {
            color: DEFAULT_TEXT;
            font-size: 8pt;
            padding:8px;
            margin-bottom:8px;
        }</ui:style>



    <g:CaptionPanel ui:field="captionField" stylePrimaryName="{localStyle.captionField}">
        <g:HTMLPanel>
            <div class="{style.css.vertical}">
                <div class="{style.css.horizontal} {style.css.alignCenter">
                    <div class="{style.css.captionText}" style="font-size:8pt; padding-left:255px">
                        <ui:msg>AM TOU</ui:msg>
                    </div>
                    <div style="width:16px"/>
                    <div class="{style.css.captionText}" style="font-size:8pt; padding-left:250px">
                        <ui:msg>PM TOU</ui:msg>
                    </div>
                </div>

                <div class="{style.css.horizontal} {style.css.alignCenter">
                    <div class="{style.css.captionText}" style="font-size:8pt; padding-left:160px">
                        <ui:msg>from</ui:msg>
                    </div>
                    <div class="{style.css.captionText}" style="font-size:8pt; padding-left:125px">
                        <ui:msg>Pto</ui:msg>
                    </div>
                    <div class="{style.css.captionText}" style="font-size:8pt; padding-left:140px">
                        <ui:msg>from</ui:msg>
                    </div>
                    <div class="{style.css.captionText}" style="font-size:8pt; padding-left:130px">
                        <ui:msg>Pto</ui:msg>
                    </div>
                </div>
                <g:VerticalPanel horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_TOP" ui:field="contentPanel"/>
            </div>
        </g:HTMLPanel>

    </g:CaptionPanel>


</ui:UiBinder>