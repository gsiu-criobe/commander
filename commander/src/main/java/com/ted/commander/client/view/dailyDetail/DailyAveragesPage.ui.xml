<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:v='urn:import:com.ted.commander.client.view'
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">

    <ui:with field="style" type="com.petecode.common.client.widget.paper.PaperStyleBundle"/>
    <ui:with field="stringRes" type="com.ted.commander.client.resources.WebStringResource"/>
    <ui:with field="imageRes" type="com.ted.commander.client.resources.DefaultImageResource"/>

    <g:HTMLPanel>
        <div class="paper-font-body1" style="padding:16px; text-align:center;width:100%" ui:field="averageText"/>
        <v:dailyDetail.DailyStatRow ui:field="kwRow">
            <ui:msg>kW</ui:msg>
        </v:dailyDetail.DailyStatRow>
        <v:dailyDetail.DailyStatRow ui:field="kwhRow">
            <ui:msg>kWh</ui:msg>
        </v:dailyDetail.DailyStatRow>
        <v:dailyDetail.DailyStatRow ui:field="costRow">
            <ui:msg>Cost</ui:msg>
        </v:dailyDetail.DailyStatRow>
        <v:dailyDetail.DailyStatRow ui:field="demandRow">
            <ui:msg>Demand</ui:msg>
        </v:dailyDetail.DailyStatRow>
        <v:dailyDetail.DailyStatRow ui:field="peakVoltageRow">
            <ui:msg>Peak Voltage</ui:msg>
        </v:dailyDetail.DailyStatRow>
        <v:dailyDetail.DailyStatRow ui:field="lowVoltageRow">
            <ui:msg>Low Voltage</ui:msg>
        </v:dailyDetail.DailyStatRow>
        <v:dailyDetail.DailyStatRow ui:field="powerFactorRow">
            <ui:msg>Power Factor</ui:msg>
        </v:dailyDetail.DailyStatRow>
        <v:dailyDetail.DailyStatRow ui:field="tempRow">
            <ui:msg>Temperature</ui:msg>
        </v:dailyDetail.DailyStatRow>
        <v:dailyDetail.DailyStatRow ui:field="windRow">
            <ui:msg>Wind</ui:msg>
        </v:dailyDetail.DailyStatRow>
        <v:dailyDetail.DailyStatRow ui:field="cloudRow">
            <ui:msg>Clouds</ui:msg>
        </v:dailyDetail.DailyStatRow>

    </g:HTMLPanel>


</ui:UiBinder>