<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:p='urn:import:com.petecode.common.client.widget.paper'
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">
    <ui:with field="paperStyleBundle" type="com.petecode.common.client.widget.paper.PaperStyleBundle"/>


    <g:HTMLPanel addStyleNames="horizontal center layout {paperStyleBundle.css.fullWidth} {paperStyleBundle.css.tile}">
        <div class="flex"></div>
        <p:PaperLabel ui:field="activationKeyField"/>
        <div class="flex"></div>
    </g:HTMLPanel>


</ui:UiBinder>